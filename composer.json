{
  "name": "old-town/workflow",
  "description": "Workflow Module",
  "type": "library",
  "license": "BSD-3-Clause",
  "keywords": [
    "workflow",
    "library"
  ],
  "minimum-stability": "dev",
  "homepage": "https://github.com/old-town/old-town-workflow",
  "support": {
    "email": "and-rey2@yandex.ru"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:old-town/old-town-propertyset.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:old-town/old-town-log.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:onema/php-coveralls.git"
    }
  ],
  "require": {
    "php": ">=5.5",
    "old-town/propertyset": "dev-dev",
    "old-town/log": "dev-dev",
    "zendframework/zend-diactoros": "1.0.1",
    "zendframework/zend-stdlib": ">=2.5.2",
    "psr/http-message": "1.0",
    "psr/log": "1.0.0"
  },
  "require-dev": {
    "ext-mbstring": "*",
    "fabpot/php-cs-fixer": "*@dev",
    "phpunit/phpunit": "*@dev",
    "zendframework/zend-loader": "*",
    "satooshi/php-coveralls": "dev-master",
    "behat/behat": "~3.0",
    "vipsoft/code-coverage-extension": "~3@dev"
  },
  "autoload": {
    "psr-4": {
      "OldTown\\Workflow\\": "src/",
      "OldTown\\Workflow\\Test\\": "test/"
    }
  }
}
